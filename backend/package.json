{
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsx": "^4.20.5",
    "typeorm": "^0.3.26",
    "typescript": "^5.9.2",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.3.0",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "dev": "tsx watch --inspect=0.0.0.0:9229 src/server.ts",
    "start": "yarn migrate:run && node ./dist/server.js",
    "build": "tsc -p .",
    "migrate:run": "node --loader ts-node/esm --env-file=../.env.dev ./node_modules/typeorm/cli.js migration:run -d dist/src/db/dataSource.js",
    "migrate:gen": "node --loader ts-node/esm --env-file=../.env.dev ./node_modules/typeorm/cli.js migration:generate --esm -d dist/src/db/dataSource.js",
    "migrate:create": "node --loader ts-node/esm --env-file=../.env.dev ./node_modules/typeorm/cli.js migration:create --esm"
  },
  "type": "module",
  "name": "backend",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "https://github.com/raspher/annapos",
  "author": "Szymon Scholz",
  "license": "MIT",
  "private": true
}
